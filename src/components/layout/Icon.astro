---
export interface Props {
  src: string;
  alt: string;
  href: string;
  width: number;
  height: number;
}

const { src, alt, href, width, height } = Astro.props;
---

<a href={href} class="hover:scale-110 transition-transform shrink-0">
  <img src={src} width={width} height={height} alt={alt} />
</a>
